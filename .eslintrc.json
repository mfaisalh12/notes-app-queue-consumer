{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-console": "off",
		"indent": [2, "tab"],
		"no-tabs": 0,
		"no-underscore-dangle": [
			"error",
			{
				"allow": ["_pool", "_transporter", "_notesService", "_mailSender"]
			}
		]
	}
}
